Private Function STATCOM(q As Double, i As Double, _
                        j As Double, B As Double) As Double
    Dim zz As Double
    Dim z As Double
    Dim k As Double
    
    zz = 1: z = zz: k = i
    Do While (k &lt;= j)
        zz = zz * q * k / (k - B)
        z = z + zz: k = k + 2
    Loop
    STATCOM = z
End Function

